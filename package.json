{
    "name": "easy_adb_fastboot",
    "version": "1.3.1",
    "description": "A GUI tool for easier using ADB and fastboot",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "author": "ryantsui1109@github",
    "license": "MIT",
    "dependencies": {
        "@popperjs/core": "^2.11.5",
        "app-builder-lib": "^23.2.0",
        "bootstrap": "^5.1.3",
        "electron-is-packaged": "^1.0.2",
        "jquery": "^3.6.0",
        "popper.js": "^1.16.1"
    },
    "devDependencies": {
        "electron": "^19.0.4",
        "electron-packager": "^15.5.1"
    },
    "build": {
        "appId": "io.github.ryantsui1109.eaf",
        "productName": "Easy ADB and fastboot",
        "icon": "./favicon_256.ico",
        "win": {
            "target": [{
                "target": "portable",

                "arch": [
                    "x64",
                    "ia32"
                ]
            }]
        }
    }
}